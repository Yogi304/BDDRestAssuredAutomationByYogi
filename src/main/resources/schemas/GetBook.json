{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Book",
  "type": "object",
  "properties": {
    "book_summary": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "published_year": {
      "type": "integer",
      "minimum": 1700
    }
  },
  "required": ["book_summary", "author", "id", "name", "published_year"]
}
